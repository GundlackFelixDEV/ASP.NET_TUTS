@model LocalBusiness.Models.Business

@{
    ViewBag.Title = Model.Titel;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div>
    <h4>@Html.DisplayFor(model => model.Titel)</h4>
</div>

<div style="padding:5px; width:100%">
    <div class="row">
        <div class="col-md-5 col-sm-5 col-xs-10">
            <div class="row">
                @{Html.RenderPartial("_BusinessContact", Model.Contact);}
            </div>
        </div>
        <div class="col-md-5 col-sm-5 col-xs-10">
            <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d5252.6282197124865!2d12.963055660064597!3d48.833146935875476!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x477511a116612765%3A0x44baa6f274515770!2sVondenhoff+Brigitte+HerrenBekleid.!5e0!3m2!1sde!2sde!4v1409123866617" frameborder="0" style="border:0"></iframe>
        </div>
    </div>
    <div class="row">
        <div class="col-md-5 col-sm-5 col-xs-10">
            @{Html.RenderPartial("_BusinessSchedule", Model.Schedule.Values.ToList());}
        </div>
    </div>
</div>

